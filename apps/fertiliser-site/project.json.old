{
    "root": "apps/fertiliser-site",
    "projectType": "application",
    "sourceRoot": "apps/fertiliser-site/src",
    "targets": {
        "build": {
            "executor": "@wanews/nx-vite:build",
            "options": {}
        },
        "serve": {
            "executor": "@wanews/nx-vite:serve",
            "options": {}
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["apps/fertiliser-site/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "options": {
                "jestConfig": "apps/fertiliser-site/jest.config.js",
                "passWithNoTests": true
            },
            "outputs": ["coverage/apps/fertiliser-site"]
        }
    },
    "tags": []
}
